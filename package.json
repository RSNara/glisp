{
  "name": "glisp",
  "version": "0.3.0",
  "description": "A simple lisp interpreter in JS.",
  "main": "lib/index.js",
  "author": "Ramanpreet Nara",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf lib",
    "commit": "git cz",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm run build",
    "repl": "node repl",
    "serve:dev": "http-server -p 8080 .",
    "prerelease": "npm test",
    "release": "standard-version",
    "postrelease": "git push --follow-tags && npm publish",
    "test": "cross-env NODE_ENV=test nyc ava",
    "test:watch": "ava -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rsnara/glisp.git"
  },
  "keywords": [
    "G",
    "LISP",
    "Interpreter"
  ],
  "files": [
    "lib",
    "index.html",
    "repl.js"
  ],
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "all": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false,
    "check-coverage": true,
    "include": [
      "src/**/*.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/rsnara/glisp/issues"
  },
  "homepage": "https://github.com/rsnara/glisp#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chalk": "^1.1.3",
    "commitizen": "^2.8.2",
    "coveralls": "^2.11.12",
    "cross-env": "^3.1.3",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^3.2.2",
    "eslint-config-rsnara": "^3.0.1",
    "http-server": "^0.9.0",
    "nyc": "^10.0.0",
    "rollup": "^0.38.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-watch": "^2.5.0",
    "standard-version": "^4.0.0",
    "testdouble": "^1.6.0"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "iterall": "^1.0.2",
    "mathjs": "^3.3.0",
    "ramda": "^0.22.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
